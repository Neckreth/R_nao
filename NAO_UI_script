library (ggplot2)
library (dplyr)
library (data.table)
library (ggpubr)
library (pastecs)

#setwd("u:/phd/r/NAO_seesaw/")
setwd("C:/Local_Science/NAO_seesaw")
#setwd("C:/Users/FABIVS/Desktop/NAO_seesaw/")

my_data=fread("NAO_UI_Fin_GL_v2.txt")

my_data_p <- my_data[(my_data$Year>1930)]
my_data_d <- my_data[(my_data$Year>1860)]

my_data_palomaa <- my_data_p[!(my_data_p$UI_palomaa=="NA")]
my_data_disko <- my_data_d[!(my_data_d$UI_disko=="NA")]


#lowpass omit everything below 0.5SD.

#UI
#----
halfSD_diskoUI <- 0.5*(sd(my_data_disko$ZUI_disko, na.rm = TRUE))
halfSD_palomaaUI <- 0.5*(sd(my_data_palomaa$ZUI_palomaa, na.rm = TRUE))

#tempanomalies
#####
#FMA temp anomaly
halfSD_diskodisko_t_ <- 0.5*(sd(my_data_disko$disko_t_FMA, na.rm = TRUE))
disko_disko_t_filtered1=my_data_disko[!(my_data_disko$disko_t_FMA > -(halfSD_diskodisko_t_) & my_data_disko$disko_t_FMA < (halfSD_diskodisko_t_)),]
disko_disko_t_filtered=disko_disko_t_filtered1[!(disko_disko_t_filtered1$ZUI_disko > -(halfSD_diskoUI) & disko_disko_t_filtered1$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaapalomaa_t_ <- 0.5*(sd(my_data_palomaa$palomaa_t_FMA, na.rm = TRUE))
palomaa_palomaa_t_filtered1=my_data_palomaa[!(my_data_palomaa$palomaa_t_FMA > -(halfSD_palomaapalomaa_t_) & my_data_palomaa$palomaa_t_FMA < (halfSD_palomaapalomaa_t_)),]
palomaa_palomaa_t_filtered=palomaa_palomaa_t_filtered1[!(palomaa_palomaa_t_filtered1$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_palomaa_t_filtered1$ZUI_palomaa < (halfSD_palomaaUI)),]



#MJJ temp anomaly
halfSD_diskodisko_t__MJJ <- 0.5*(sd(my_data_disko$disko_t_MJJ, na.rm = TRUE))
disko_disko_t_filtered1_MJJ=my_data_disko[!(my_data_disko$disko_t_MJJ > -(halfSD_diskodisko_t__MJJ) & my_data_disko$disko_t_MJJ < (halfSD_diskodisko_t__MJJ)),]
disko_disko_t_filtered_MJJ=disko_disko_t_filtered1_MJJ[!(disko_disko_t_filtered1_MJJ$ZUI_disko > -(halfSD_diskoUI) & disko_disko_t_filtered1_MJJ$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaapalomaa_t__MJJ <- 0.5*(sd(my_data_palomaa$palomaa_t_MJJ, na.rm = TRUE))
palomaa_palomaa_t_filtered1_MJJ=my_data_palomaa[!(my_data_palomaa$palomaa_t_MJJ > -(halfSD_palomaapalomaa_t__MJJ) & my_data_palomaa$palomaa_t_MJJ < (halfSD_palomaapalomaa_t_)),]
palomaa_palomaa_t_filtered_MJJ=palomaa_palomaa_t_filtered1_MJJ[!(palomaa_palomaa_t_filtered1_MJJ$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_palomaa_t_filtered1_MJJ$ZUI_palomaa < (halfSD_palomaaUI)),]

#NDJF temp anomaly
halfSD_diskodisko_t__NDJF <- 0.5*(sd(my_data_disko$disko_t_NDJF, na.rm = TRUE))
disko_disko_t_filtered1_NDJF=my_data_disko[!(my_data_disko$disko_t_NDJF > -(halfSD_diskodisko_t__NDJF) & my_data_disko$disko_t_NDJF < (halfSD_diskodisko_t__NDJF)),]
disko_disko_t_filtered_NDJF=disko_disko_t_filtered1_NDJF[!(disko_disko_t_filtered1_NDJF$ZUI_disko > -(halfSD_diskoUI) & disko_disko_t_filtered1_NDJF$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaapalomaa_t__NDJF <- 0.5*(sd(my_data_palomaa$palomaa_t_NDJF, na.rm = TRUE))
palomaa_palomaa_t_filtered1_NDJF=my_data_palomaa[!(my_data_palomaa$palomaa_t_NDJF > -(halfSD_palomaapalomaa_t__NDJF) & my_data_palomaa$palomaa_t_NDJF < (halfSD_palomaapalomaa_t_)),]
palomaa_palomaa_t_filtered_NDJF=palomaa_palomaa_t_filtered1_NDJF[!(palomaa_palomaa_t_filtered1_NDJF$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_palomaa_t_filtered1_NDJF$ZUI_palomaa < (halfSD_palomaaUI)),]

#AM temp anomaly
halfSD_diskodisko_t__AM <- 0.5*(sd(my_data_disko$disko_t_AM, na.rm = TRUE))
disko_disko_t_filtered1_AM=my_data_disko[!(my_data_disko$disko_t_AM > -(halfSD_diskodisko_t__AM) & my_data_disko$disko_t_AM < (halfSD_diskodisko_t__AM)),]
disko_disko_t_filtered_AM=disko_disko_t_filtered1_AM[!(disko_disko_t_filtered1_AM$ZUI_disko > -(halfSD_diskoUI) & disko_disko_t_filtered1_AM$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaapalomaa_t__AM <- 0.5*(sd(my_data_palomaa$palomaa_t_AM, na.rm = TRUE))
palomaa_palomaa_t_filtered1_AM=my_data_palomaa[!(my_data_palomaa$palomaa_t_AM > -(halfSD_palomaapalomaa_t__AM) & my_data_palomaa$palomaa_t_AM < (halfSD_palomaapalomaa_t_)),]
palomaa_palomaa_t_filtered_AM=palomaa_palomaa_t_filtered1_AM[!(palomaa_palomaa_t_filtered1_AM$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_palomaa_t_filtered1_AM$ZUI_palomaa < (halfSD_palomaaUI)),]


#####
#FMA
halfSD_diskoNAO2 <- 0.5*(sd(my_data_disko$NAO2FMA, na.rm = TRUE))
disko_NAO2filtered1=my_data_disko[!(my_data_disko$NAO2FMA > -(halfSD_diskoNAO2) & my_data_disko$NAO2FMA < (halfSD_diskoNAO2)),]
disko_NAO2filtered=disko_NAO2filtered1[!(disko_NAO2filtered1$ZUI_disko > -(halfSD_diskoUI) & disko_NAO2filtered1$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO2 <- 0.5*(sd(my_data_palomaa$NAO2FMA, na.rm = TRUE))
palomaa_NAO2filtered1=my_data_palomaa[!(my_data_palomaa$NAO2FMA > -(halfSD_palomaaNAO2) & my_data_palomaa$NAO2FMA < (halfSD_palomaaNAO2)),]
palomaa_NAO2filtered=palomaa_NAO2filtered1[!(palomaa_NAO2filtered1$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO2filtered1$ZUI_palomaa < (halfSD_palomaaUI)),]

halfSD_diskoNAO1 <- 0.5*(sd(my_data_disko$NAO1FMA, na.rm = TRUE))
disko_NAO1filtered1=my_data_disko[!(my_data_disko$NAO1FMA > -(halfSD_diskoNAO1) & my_data_disko$NAO1FMA < (halfSD_diskoNAO1)),]
disko_NAO1filtered=disko_NAO1filtered1[!(disko_NAO1filtered1$ZUI_disko > -(halfSD_diskoUI) & disko_NAO1filtered1$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO1 <- 0.5*(sd(my_data_palomaa$NAO1FMA, na.rm = TRUE))
palomaa_NAO1filtered1=my_data_palomaa[!(my_data_palomaa$NAO1FMA > -(halfSD_palomaaNAO1) & my_data_palomaa$NAO1FMA < (halfSD_palomaaNAO1)),]
palomaa_NAO1filtered=palomaa_NAO1filtered1[!(palomaa_NAO1filtered1$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO1filtered1$ZUI_palomaa < (halfSD_palomaaUI)),]



#MJJ
halfSD_diskoNAO2_MJJ <- 0.5*(sd(my_data_disko$NAO2MJJ, na.rm = TRUE))
disko_NAO2filtered1_MJJ=my_data_disko[!(my_data_disko$NAO2MJJ > -(halfSD_diskoNAO2_MJJ) & my_data_disko$NAO2MJJ < (halfSD_diskoNAO2_MJJ)),]
disko_NAO2filtered_MJJ=disko_NAO2filtered1_MJJ[!(disko_NAO2filtered1_MJJ$ZUI_disko > -(halfSD_diskoUI) & disko_NAO2filtered1_MJJ$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO2_MJJ <- 0.5*(sd(my_data_palomaa$NAO2MJJ, na.rm = TRUE))
palomaa_NAO2filtered1_MJJ=my_data_palomaa[!(my_data_palomaa$NAO2MJJ > -(halfSD_palomaaNAO2_MJJ) & my_data_palomaa$NAO2MJJ < (halfSD_palomaaNAO2_MJJ)),]
palomaa_NAO2filtered_MJJ=palomaa_NAO2filtered1_MJJ[!(palomaa_NAO2filtered1_MJJ$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO2filtered1_MJJ$ZUI_palomaa < (halfSD_palomaaUI)),]

halfSD_diskoNAO1_MJJ <- 0.5*(sd(my_data_disko$NAO1MJJ, na.rm = TRUE))
disko_NAO1filtered1_MJJ=my_data_disko[!(my_data_disko$NAO1MJJ > -(halfSD_diskoNAO1_MJJ) & my_data_disko$NAO1MJJ < (halfSD_diskoNAO1_MJJ)),]
disko_NAO1filtered_MJJ=disko_NAO1filtered1_MJJ[!(disko_NAO1filtered1_MJJ$ZUI_disko > -(halfSD_diskoUI) & disko_NAO1filtered1_MJJ$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO1_MJJ <- 0.5*(sd(my_data_palomaa$NAO1MJJ, na.rm = TRUE))
palomaa_NAO1filtered1_MJJ=my_data_palomaa[!(my_data_palomaa$NAO1MJJ > -(halfSD_palomaaNAO1_MJJ) & my_data_palomaa$NAO1MJJ < (halfSD_palomaaNAO1)),]
palomaa_NAO1filtered_MJJ=palomaa_NAO1filtered1_MJJ[!(palomaa_NAO1filtered1_MJJ$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO1filtered1_MJJ$ZUI_palomaa < (halfSD_palomaaUI)),]

#NDJF
halfSD_diskoNAO2_NDJF <- 0.5*(sd(my_data_disko$NAO2NDJF, na.rm = TRUE))
disko_NAO2filtered1_NDJF=my_data_disko[!(my_data_disko$NAO2NDJF > -(halfSD_diskoNAO2_NDJF) & my_data_disko$NAO2NDJF < (halfSD_diskoNAO2_NDJF)),]
disko_NAO2filtered_NDJF=disko_NAO2filtered1_NDJF[!(disko_NAO2filtered1_NDJF$ZUI_disko > -(halfSD_diskoUI) & disko_NAO2filtered1_NDJF$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO2_NDJF <- 0.5*(sd(my_data_palomaa$NAO2NDJF, na.rm = TRUE))
palomaa_NAO2filtered1_NDJF=my_data_palomaa[!(my_data_palomaa$NAO2NDJF > -(halfSD_palomaaNAO2_NDJF) & my_data_palomaa$NAO2NDJF < (halfSD_palomaaNAO2_NDJF)),]
palomaa_NAO2filtered_NDJF=palomaa_NAO2filtered1_NDJF[!(palomaa_NAO2filtered1_NDJF$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO2filtered1_NDJF$ZUI_palomaa < (halfSD_palomaaUI)),]

halfSD_diskoNAO1_NDJF <- 0.5*(sd(my_data_disko$NAO1NDJF, na.rm = TRUE))
disko_NAO1filtered1_NDJF=my_data_disko[!(my_data_disko$NAO1NDJF > -(halfSD_diskoNAO1_NDJF) & my_data_disko$NAO1NDJF < (halfSD_diskoNAO1_NDJF)),]
disko_NAO1filtered_NDJF=disko_NAO1filtered1_NDJF[!(disko_NAO1filtered1_NDJF$ZUI_disko > -(halfSD_diskoUI) & disko_NAO1filtered1_NDJF$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO1_NDJF <- 0.5*(sd(my_data_palomaa$NAO1NDJF, na.rm = TRUE))
palomaa_NAO1filtered1_NDJF=my_data_palomaa[!(my_data_palomaa$NAO1NDJF > -(halfSD_palomaaNAO1_NDJF) & my_data_palomaa$NAO1NDJF < (halfSD_palomaaNAO1)),]
palomaa_NAO1filtered_NDJF=palomaa_NAO1filtered1_NDJF[!(palomaa_NAO1filtered1_NDJF$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO1filtered1_NDJF$ZUI_palomaa < (halfSD_palomaaUI)),]

#AM
halfSD_diskoNAO2_AM <- 0.5*(sd(my_data_disko$NAO2AM, na.rm = TRUE))
disko_NAO2filtered1_AM=my_data_disko[!(my_data_disko$NAO2AM > -(halfSD_diskoNAO2_AM) & my_data_disko$NAO2AM < (halfSD_diskoNAO2_AM)),]
disko_NAO2filtered_AM=disko_NAO2filtered1_AM[!(disko_NAO2filtered1_AM$ZUI_disko > -(halfSD_diskoUI) & disko_NAO2filtered1_AM$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO2_AM <- 0.5*(sd(my_data_palomaa$NAO2AM, na.rm = TRUE))
palomaa_NAO2filtered1_AM=my_data_palomaa[!(my_data_palomaa$NAO2AM > -(halfSD_palomaaNAO2_AM) & my_data_palomaa$NAO2AM < (halfSD_palomaaNAO2_AM)),]
palomaa_NAO2filtered_AM=palomaa_NAO2filtered1_AM[!(palomaa_NAO2filtered1_AM$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO2filtered1_AM$ZUI_palomaa < (halfSD_palomaaUI)),]

halfSD_diskoNAO1_AM <- 0.5*(sd(my_data_disko$NAO1AM, na.rm = TRUE))
disko_NAO1filtered1_AM=my_data_disko[!(my_data_disko$NAO1AM > -(halfSD_diskoNAO1_AM) & my_data_disko$NAO1AM < (halfSD_diskoNAO1_AM)),]
disko_NAO1filtered_AM=disko_NAO1filtered1_AM[!(disko_NAO1filtered1_AM$ZUI_disko > -(halfSD_diskoUI) & disko_NAO1filtered1_AM$ZUI_disko < (halfSD_diskoUI)),]

halfSD_palomaaNAO1_AM <- 0.5*(sd(my_data_palomaa$NAO1AM, na.rm = TRUE))
palomaa_NAO1filtered1_AM=my_data_palomaa[!(my_data_palomaa$NAO1AM > -(halfSD_palomaaNAO1_AM) & my_data_palomaa$NAO1AM < (halfSD_palomaaNAO1)),]
palomaa_NAO1filtered_AM=palomaa_NAO1filtered1_AM[!(palomaa_NAO1filtered1_AM$ZUI_palomaa > -(halfSD_palomaaUI) & palomaa_NAO1filtered1_AM$ZUI_palomaa < (halfSD_palomaaUI)),]




##### 
#statistics 

#disko no filter vs temperature anomalies
Disko_nofilter_vs_disko_t_ <- cor.test(my_data$UI_disko, my_data$disko_t_average, method = "pearson")
Disko_nofilter_vs_disko_t__pearson <- Disko_nofilter_vs_disko_t_[["estimate"]][["cor"]]
Disko_nofilter_vs_disko_t__p_value <- Disko_nofilter_vs_disko_t_[["p.value"]]
Disko_nofilter_vs_disko_t__df_value <- Disko_nofilter_vs_disko_t_[["parameter"]][["df"]]
format(round(Disko_nofilter_vs_disko_t__pearson, 3), nsmall = 3) -> Disko_nofilter_vs_disko_t__pearson_ab
format(round(Disko_nofilter_vs_disko_t__p_value, 4), nsmall = 4) -> Disko_nofilter_vs_disko_t__p_value_ab
Disko_nofilter_vs_disko_t__df_value + 2 -> Disko_nofilter_vs_disko_t__n_value

#palomaa no filter vs temperature anomalies
palomaa_nofilter_vs_palomaa_t_ <- cor.test(my_data$UI_palomaa, my_data$palomaa_t_average, method = "pearson")
palomaa_nofilter_vs_palomaa_t__pearson <- palomaa_nofilter_vs_palomaa_t_[["estimate"]][["cor"]]
palomaa_nofilter_vs_palomaa_t__p_value <- palomaa_nofilter_vs_palomaa_t_[["p.value"]]
palomaa_nofilter_vs_palomaa_t__df_value <- palomaa_nofilter_vs_palomaa_t_[["parameter"]][["df"]]
format(round(palomaa_nofilter_vs_palomaa_t__pearson, 3), nsmall = 3) -> palomaa_nofilter_vs_palomaa_t__pearson_ab
format(round(palomaa_nofilter_vs_palomaa_t__p_value, 4), nsmall = 4) -> palomaa_nofilter_vs_palomaa_t__p_value_ab
palomaa_nofilter_vs_palomaa_t__df_value +2 -> palomaa_nofilter_vs_palomaa_t__n_value

#NAO1 vs Disko temperature anomalies
NAO1_vs_disko_t_ <- cor.test(my_data$NAO1Average, my_data$disko_t_average, method = "pearson")
NAO1_vs_disko_t__pearson <- NAO1_vs_disko_t_[["estimate"]][["cor"]]
NAO1_vs_disko_t__p_value <- NAO1_vs_disko_t_[["p.value"]]
NAO1_vs_disko_t__df_value <- NAO1_vs_disko_t_[["parameter"]][["df"]]
format(round(NAO1_vs_disko_t__pearson, 3), nsmall = 3) -> NAO1_vs_disko_t__pearson_ab
format(round(NAO1_vs_disko_t__p_value, 4), nsmall = 4) -> NAO1_vs_disko_t__p_value_ab
NAO1_vs_disko_t__df_value + 2 -> NAO1_vs_disko_t__n_value

#NAO1 vs Palomaa temperature anomalies
NAO1_vs_palomaa_t_ <- cor.test(my_data$NAO1Average, my_data$palomaa_t_average, method = "pearson")
NAO1_vs_palomaa_t__pearson <- NAO1_vs_palomaa_t_[["estimate"]][["cor"]]
NAO1_vs_palomaa_t__p_value <- NAO1_vs_palomaa_t_[["p.value"]]
NAO1_vs_palomaa_t__df_value <- NAO1_vs_palomaa_t_[["parameter"]][["df"]]
format(round(NAO1_vs_palomaa_t__pearson, 3), nsmall = 3) -> NAO1_vs_palomaa_t__pearson_ab
format(round(NAO1_vs_palomaa_t__p_value, 4), nsmall = 4) -> NAO1_vs_palomaa_t__p_value_ab
NAO1_vs_palomaa_t__df_value +2 -> NAO1_vs_palomaa_t__n_value

#NAO2 vs Disko temperature anomalies
NAO2_vs_disko_t_ <- cor.test(my_data$NAO2Average, my_data$disko_t_average, method = "pearson")
NAO2_vs_disko_t__pearson <- NAO2_vs_disko_t_[["estimate"]][["cor"]]
NAO2_vs_disko_t__p_value <- NAO2_vs_disko_t_[["p.value"]]
NAO2_vs_disko_t__df_value <- NAO2_vs_disko_t_[["parameter"]][["df"]]
format(round(NAO2_vs_disko_t__pearson, 3), nsmall = 3) -> NAO2_vs_disko_t__pearson_ab
format(round(NAO2_vs_disko_t__p_value, 4), nsmall = 4) -> NAO2_vs_disko_t__p_value_ab
NAO2_vs_disko_t__df_value + 2 -> NAO2_vs_disko_t__n_value

