format(round(NAO2_vs_palomaa_t__p_value, 4), nsmall = 4) -> NAO2_vs_palomaa_t__p_value_ab
NAO2_vs_palomaa_t__df_value +2 -> NAO2_vs_palomaa_t__n_value

#NAO1FMA vs Disko temperature anomalies
