palomaa_NAO2_pearsonAM <- cor.test(palomaa_NAO2filtered_AM$UI_palomaa, palomaa_NAO2filtered_AM$NAO2AM, method = "pearson" )
disko_NAO1_pearsonAM <- cor.test(disko_NAO1filtered_AM$UI_disko, disko_NAO1filtered_AM$NAO1AM, method = "pearson" )
palomaa_NAO1_pearsonAM <- cor.test(palomaa_NAO1filtered_AM$UI_palomaa, palomaa_NAO1filtered_AM$NAO1AM, method = "pearson" )


