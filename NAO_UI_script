#FMA
disko_NAO2_pearsonFMA <- cor.test(disko_NAO2filtered$UI_disko, disko_NAO2filtered$NAO2FMA, method = "pearson" )
palomaa_NAO2_pearsonFMA <- cor.test(palomaa_NAO2filtered$UI_palomaa, palomaa_NAO2filtered$NAO2FMA, method = "pearson" )
disko_NAO1_pearsonFMA <- cor.test(disko_NAO1filtered$UI_disko, disko_NAO1filtered$NAO1FMA, method = "pearson" )
palomaa_NAO1_pearsonFMA <- cor.test(palomaa_NAO1filtered$UI_palomaa, palomaa_NAO1filtered$NAO1FMA, method = "pearson" )

#MJJ
disko_NAO2_pearsonMJJ <- cor.test(disko_NAO2filtered_MJJ$UI_disko, disko_NAO2filtered_MJJ$NAO2MJJ, method = "pearson" )
palomaa_NAO2_pearsonMJJ <- cor.test(palomaa_NAO2filtered_MJJ$UI_palomaa, palomaa_NAO2filtered_MJJ$NAO2MJJ, method = "pearson" )
disko_NAO1_pearsonMJJ <- cor.test(disko_NAO1filtered_MJJ$UI_disko, disko_NAO1filtered_MJJ$NAO1MJJ, method = "pearson" )
palomaa_NAO1_pearsonMJJ <- cor.test(palomaa_NAO1filtered_MJJ$UI_palomaa, palomaa_NAO1filtered_MJJ$NAO1MJJ, method = "pearson" )

#NDJF
disko_NAO2_pearsonNDJF <- cor.test(disko_NAO2filtered_NDJF$UI_disko, disko_NAO2filtered_NDJF$NAO2NDJF, method = "pearson" )
palomaa_NAO2_pearsonNDJF <- cor.test(palomaa_NAO2filtered_NDJF$UI_palomaa, palomaa_NAO2filtered_NDJF$NAO2NDJF, method = "pearson" )
disko_NAO1_pearsonNDJF <- cor.test(disko_NAO1filtered_NDJF$UI_disko, disko_NAO1filtered_NDJF$NAO1NDJF, method = "pearson" )
palomaa_NAO1_pearsonNDJF <- cor.test(palomaa_NAO1filtered_NDJF$UI_palomaa, palomaa_NAO1filtered_NDJF$NAO1NDJF, method = "pearson" )

#AM
