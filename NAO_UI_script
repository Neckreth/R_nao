#NAO2FMA vs Palomaa temperature anomalies
NAO2FMA_vs_palomaa_t_ <- cor.test(my_data$NAO2FMA, my_data$palomaa_t_average, method = "pearson")
NAO2FMA_vs_palomaa_t__pearson <- NAO2FMA_vs_palomaa_t_[["estimate"]][["cor"]]
NAO2FMA_vs_palomaa_t__p_value <- NAO2FMA_vs_palomaa_t_[["p.value"]]
NAO2FMA_vs_palomaa_t__df_value <- NAO2FMA_vs_palomaa_t_[["parameter"]][["df"]]
format(round(NAO2FMA_vs_palomaa_t__pearson, 3), nsmall = 3) -> NAO2FMA_vs_palomaa_t__pearson_ab
format(round(NAO2FMA_vs_palomaa_t__p_value, 4), nsmall = 4) -> NAO2FMA_vs_palomaa_t__p_value_ab
NAO2FMA_vs_palomaa_t__df_value +2 -> NAO2FMA_vs_palomaa_t__n_value

