#plot disko
plotZUIdisko <- ggplot() + 
  geom_col(data = disko_NAO1filtered, aes(x = Year, y = ZUI_disko), alpha = 5/10, shape = 21, fill = "purple", color = "purple", size=4, position = "dodge") +
  geom_col(data = disko_NAO2filtered, aes(x = Year, y = ZUI_disko), alpha = 5/10, shape = 21, fill = "purple", color = "purple", size=4, position = "dodge") +
  geom_col(data = disko_NAO1filtered, aes(x = Year, y = NAO1FMA), alpha = 5/10, shape=23, fill="black", color="black", size=4, position = "dodge") +
  geom_col(data = disko_NAO2filtered, aes(x = Year, y = NAO2FMA), alpha = 5/10, shape=23, fill="darkgrey", color="darkgrey", size=4, position = "dodge") +
  geom_point(data = disko_NAO1filtered, aes(x = Year, y = ZUI_disko),shape = 21, fill = "purple", color = "orange", size=4) +
  geom_point(data = disko_NAO2filtered, aes(x = Year, y = ZUI_disko),shape = 21, fill = "purple", color = "orange", size=4) +
  geom_point(data = disko_NAO1filtered, aes(x = Year, y = NAO1FMA), shape=23, fill="black", color="darkgrey", size=4) +
  geom_point(data = disko_NAO2filtered, aes(x = Year, y = NAO2FMA), shape=23, fill="darkgrey", color="black", size=4) +
  geom_hline(yintercept=0, linetype="dashed", color = "black")+
  geom_hline(yintercept= halfSD_diskoNAO2 , linetype="dashed", color = "grey")+
  geom_hline(yintercept= - (halfSD_diskoNAO2), linetype="dashed", color = "grey")+
  geom_hline(yintercept= halfSD_diskoNAO1 , linetype="dashed", color = "darkgrey")+
  geom_hline(yintercept= - (halfSD_diskoNAO1), linetype="dashed", color = "darkgrey")+
  geom_hline(yintercept= halfSD_diskoUI , linetype="dashed", color = "purple")+
  geom_hline(yintercept= - (halfSD_diskoUI), linetype="dashed", color = "purple")+
  
  theme_bw() + theme(
    aspect.ratio=0.5,
    panel.border = element_blank(), 
    panel.grid.major = element_blank(),
    panel.grid.minor = element_blank(), 
    axis.line = element_line(colour = "black"),
    axis.text.x = element_text(face="bold", color="black", 
                               size=12, angle=90),
    axis.text.y = element_text(face="bold", color="black", 
                               size=12, angle=0))+
  scale_x_continuous(breaks = seq(1830, 2030, by = 10))+
  annotate("text", x = 1920, y = 2.0, label = disko_NAO1FMA_p_value_ab)+
  annotate("text", x = 1980, y = 2.0, label = disko_NAO2FMA_p_value_ab)+
  annotate("text", x = 1920, y = 2.25, label = disko_NAO1FMA_pearson_ab)+
  annotate("text", x = 1980, y = 2.25, label = disko_NAO2FMA_pearson_ab)+
  annotate("text", x = 1920, y = 2.5, label = "correlation (R) with nao1FMA and p")+
  annotate("text", x = 1980, y = 2.5, label = "correlation (R) with nao2FMA and p")

