plotUIraw <- ggplot() + 
  geom_point(data = my_data_p, aes(x = Year, y = UI_palomaa), shape = 21, fill = "orange", color = "purple", size=4) +
  geom_point(data = my_data_d, aes(x = Year, y = UI_disko), shape = 21, fill = "purple", color = "orange", size=4) +
  labs(x = "Year CE",
       y = "UI Palomaa & Disko",
       color = "Legend")  +
  annotate("text", x = 1900, y = 1.3, label = "Purple = Disko \n Orange = Palomaa")+
  annotate("text", x = 1885, y = 1.25, label = "Palomaa mean=")+
  annotate("text", x = 1900, y = 1.25, label = mean_UI_palomaa)+
  annotate("text", x = 1915, y = 1.25, label = "sd=")+
  annotate("text", x = 1920, y = 1.25, label = sd_UI_palomaa)+
  annotate("text", x = 1885, y = 1.23, label = "Disko mean=")+
  annotate("text", x = 1900, y = 1.23, label = mean_UI_disko)+
  annotate("text", x = 1915, y = 1.23, label = "sd=")+
  annotate("text", x = 1920, y = 1.23, label = sd_UI_disko)+
  
  
  
  theme_bw() +  theme(
    aspect.ratio=0.5,
    panel.border = element_blank(), 
    panel.grid.major = element_blank(),
    panel.grid.minor = element_blank(), 
    axis.line = element_line(colour = "black"))+
  scale_x_continuous(breaks = seq(1830, 2030, by = 20))


